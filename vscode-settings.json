{
    // "terminal.integrated.fontFamily": "Meslo LG S for Powerline",
    // "terminal.integrated.fontSize": 12,
    "editor.tabSize": 2,
    "editor.renderWhitespace": "all",
    "editor.minimap.enabled": false,
    "workbench.editor.revealIfOpen": true,
    "files.insertFinalNewline": true,
    "gitlens.keymap": "chorded",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
    },
    "ruby.codeCompletion": false,
    "ruby.intellisense": false,
    "ruby.locate": {
        "include": "**/*.rb",
        "exclude": "{**/@(test|spec|tmp|.*),**/@(test|spec|tmp|.*)/**,**/*_spec.rb}"
    },
    "ruby.format": false,
    "ruby.rubocop.onSave": true,
    "ruby.lintDebounceTime": 800,
    "editor.multiCursorModifier": "alt",
    "workbench.editor.enablePreview": false,
    "files.associations": {
        "*.scss.liquid": "scss",
        "*.js.liquid": "javascript",
        "*.liquid": "liquid"
    },
    "gitlens.views.fileHistory.enabled": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "byesig.backgroundColor": "#1e1e1e",
    "byesig.opacity": 0.5,
    "go.formatTool": "goimports",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "byesig.fold": false,
    "editor.accessibilitySupport": "off",
    "editor.inlineSuggest.enabled": true,
    "ruby.useLanguageServer": false,
    "ruby.lint": {
        "rubocop": false
    },
    "[ruby]": {
        "editor.defaultFormatter": "Shopify.ruby-lsp",
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.semanticHighlighting.enabled": true
    },
    "byesig.enabled": true,
    "byesig.showIcon": false,
    "files.trimTrailingWhitespace": true,
    "shopifyLiquid.formatterDevPreview": true,
    "[liquid]": {
        "editor.defaultFormatter": "Shopify.theme-check-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.rulers": [
        120
    ],
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "themeCheck.checkOnChange": false,
    "themeCheck.checkOnOpen": false,
    "themeCheck.checkOnSave": false,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Spinel",
    "remote.SSH.connectTimeout": 150
}
